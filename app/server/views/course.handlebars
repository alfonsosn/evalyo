<professor-title>

  <h1 class="mdc-typography--headline">{{professor.firstName}} {{professor.lastName}}</h1>
  <base href="./{{professor.firstName}}/">
  <h1 class="mdc-typography--headline">{{course.semester}}, {{course.subject}}</h1>
  <base href="./{{course.subject}}/" />
</professor-title>

<div class="mdc-card demo-card">
  <section class="mdc-card__primary mdc-typography--title">
    Clarity:
  </section>
  <ul class="mdc-list">
    {{#clarity}}
      <li class="mdc-list-item">
      <b>{{question}} </b> : 
         {{#if average}}  
           {{average}}
         {{else}} 
           yes: {{yes}}
           no: {{no}}
         {{/if}}
      </li>
    {{/clarity}}
  </ul>
  <section class="mdc-card__primary mdc-typography--title">
    Organization:
  </section>
  <ul class="mdc-list">
    {{#organization}}
      <li class="mdc-list-item">
      <b>{{question}} </b> : 
         {{#if average}}  
           {{average}}
         {{else}} 
           yes: {{yes}}
           no: {{no}}
         {{/if}}
      </li>
    {{/organization}}
  </ul>
  <section class="mdc-card__primary mdc-typography--title">
    Experience:
  </section>
  <ul class="mdc-list">
    {{#experience}}
      <li class="mdc-list-item">
      <b>{{question}} </b> : 
         {{#if average}}  
           {{average}}
         {{else}} 
           yes: {{yes}}
           no: {{no}}
         {{/if}}
      </li>
    {{/experience}}
  </ul>
  <section class="mdc-card__primary mdc-typography--title">
    Personality:
  </section>
  <ul class="mdc-list">
    {{#personality}}
      <li class="mdc-list-item">
      <b>{{question}} </b> : 
         {{#if average}}  
           {{average}}
         {{else}} 
           yes: {{yes}}
           no: {{no}}
         {{/if}}
      </li>
    {{/personality}}
  </ul>
</div>
