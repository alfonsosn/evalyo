<professor-title>
  <h1 class="mdc-typography--headline">{{professor.firstName}} {{professor.lastName}}</h1>
  <base href="./{{professor.firstName}}/">
  <h1 class="mdc-typography--headline">{{course.subject}}</h1>
  <base href="./{{course.subject}}/" />
</professor-title>

{{#each evaluations}}
{{#each this}}
  <semester>
    <h2> <a style="cursor:pointer; text-decoration: underline;color: blue;">{{this.semester}}</a></h2>
    <div id="review" class="hide" class="mdc-card demo-card">

      {{!Clarity Section}}
      <section class="mdc-card__primary mdc-typography--title">Clarity</section>
      <ul class="mdc-list">
        <table>
        {{#this.clarity}}
          <li>
         <tr>
           <td> <b> {{question}} </b> </td>
             <td> {{#if average}} </td>
              <td> {{average}} </td>
           <td>  {{else}} </td>
            <td>   Yes: {{yes}} </td>
              <td> No: {{no}} </td>
         </tr>
             {{/if}}
          </li>
        {{/this.clarity}}
        </table>
      </ul>


       {{!Organization Section}}
      <section class="mdc-card__primary mdc-typography--title">Organization</section>
      <ul class="mdc-list">
        <table>
        {{#this.organization}}
          <li>
          <tr>
           <td> <b> {{question}} </b> </td>
             <td> {{#if average}} </td>
              <td> {{average}} </td>
           <td>  {{else}} </td>
            <td>   Yes: {{yes}} </td>
              <td> No: {{no}} </td>
         </tr>
             {{/if}}
          </li>
        {{/this.organization}}
        </table>
      </ul>

       {{!Experience Section}}
      <section class="mdc-card__primary mdc-typography--title">Experience</section>
      <ul class="mdc-list">
        <table>
        {{#this.experience}}
          <li>
          <tr>
           <td> <b> {{question}} </b> </td>
             <td> {{#if average}} </td>
              <td> {{average}} </td>
           <td>  {{else}} </td>
            <td>   Yes: {{yes}} </td>
              <td> No: {{no}} </td>
         </tr>
             {{/if}}
          </li>
        {{/this.experience}}
        </table>
      </ul>

       {{!Personality Section}}
      <section class="mdc-card__primary mdc-typography--title">Personality</section>
      <ul class="mdc-list">
        <table>
        {{#this.personality}}
          <li>
         <tr>
           <td> <b> {{question}} </b> </td>
             <td> {{#if average}} </td>
              <td> {{average}} </td>
           <td>  {{else}} </td>
            <td>   Yes: {{yes}} </td>
              <td> No: {{no}} </td>
         </tr>
             {{/if}}
          </li>
        {{/this.personality}}
        </table>
      </ul>
    </div>
  </semester>
  {{/each}}
{{/each}}
